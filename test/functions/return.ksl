

fun main(): uint32
    printunsigned(thingy2(1, 2))          // expect output 3
    printunsigned(thingy3(1, 2, 3))       // expect output 6
    printunsigned(thingy4(1, 2, 3, 4))    // expect output 10
    printunsigned(thingy5(1, 2, 3, 4, 5)) // expect output 15
    ^0

fun printunsigned(uint32 i): void

fun thingy2(uint32 a, uint32 b): uint32
    ^a + b
fun thingy3(uint32 a, uint32 b, uint32 c): uint32
    ^a + b + c
fun thingy4(uint32 a, uint32 b, uint32 c, uint32 d): uint32
    ^a + b + c + d
fun thingy5(uint32 a, uint32 b, uint32 c, uint32 d, uint32 e): uint32
    ^a + b + c + d + e
