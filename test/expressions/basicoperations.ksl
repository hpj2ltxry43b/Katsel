

fun uint32 main()
    printunsigned(1 + 1) // expect output 2
    printunsigned(1 - 1) // expect output 0
    printunsigned(1 * 1) // expect output 1
    printunsigned(1 / 1) // expect output 1
    printunsigned(1 % 1) // expect output 0

    printunsigned(42 + 55) // expect output 97
    printunsigned(42 - 23) // expect output 19
    printunsigned(42 * 75) // expect output 3150
    printunsigned(42 / 4) // expect output 10
    printfloat(42.0 / 4.0) // expect output 10.5
    printunsigned(42 % 5) // expect output 2

    printunsigned(-42 / 4) // expect output 1073741813
    printfloat(-42.0 / 4.0) // expect output -10.5
    printunsigned(-42 % 5) // expect output 4
    <- 0

fun void printunsigned(uint32 d)
fun void printfloat(float d)
