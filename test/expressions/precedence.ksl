

fun uint32 main()
{
    printunsigned(2 + 2 * 3); // expect output 8
    printunsigned((2 + 2) * 3); // expect output 12

    printunsigned(2 / 2 * 3); // expect output 3
    printunsigned((2 / 2) * 3); // expect output 3
    printunsigned(2 / (2 * 3)); // expect output 0
}

fun void printunsigned(uint32 i);
